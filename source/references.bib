@article{bartokClimateProjectionDataset2019,
  title = {A Climate Projection Dataset Tailored for the {{European}} Energy Sector},
  author = {Bart{\'o}k, Blanka and Tobin, Isabelle and Vautard, Robert and Vrac, Mathieu and Jin, Xia and Levavasseur, Guillaume and Denvil, S{\'e}bastien and Dubus, Laurent and Parey, Sylvie and Michelangeli, Paul-Antoine and Troccoli, Alberto and {Saint-Drenan}, Yves-Marie},
  year = {2019},
