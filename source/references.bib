@article{craigOvercomingDisconnectEnergy2022,
  title = {Overcoming the Disconnect between Energy System and Climate Modeling},
  author = {Craig, Michael T. and Wohland, Jan and Stoop, Laurens P. and Kies, Alexander and Pickering, Bryn and Bloomfield, Hannah C. and Browell, Jethro and De Felice, Matteo and Dent, Chris J. and Deroubaix, Adrien and Frischmuth, Felix and Gonzalez, Paula L.M. and Grochowicz, Aleksander and Gruber, Katharina and H{\"a}rtel, Philipp and Kittel, Martin and Kotzur, Leander and Labuhn, Inga and Lundquist, Julie K. and Pflugradt, Noah and {van der Wiel}, Karin and Zeyringer, Marianne and Brayshaw, David J.},
